@InProceedings{Soundararajan2005,
  author    = {Soundararajan, Gokul and Manassiev, Kaloian and Chen, Jin and Goel, Ashvin and Amza, Cristiana},
  booktitle = {Proceedings of the Second International Conference on Automatic Computing},
  date      = {2005},
  title     = {Feedback-Based Scheduling for Back-End Databases in Shared Dynamic Content Server Clusters},
  doi       = {10.1109/ICAC.2005.29},
  isbn      = {0796522769},
  location  = {USA},
  pages     = {348–349},
